         <button style=\"margin:3px;\" @click=\"onbuttonclick('addString')\">文档开头添加字符串</button>\r\n            <button style=\"margin:3px;\" @click=\"onbuttonclick('closeDoc')\">关闭文件</button>\r\n        </div>\r\n        <hr>\r\n        <div class=\"divItem\">文档文件名为：<span>{{docName}}</span></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport dlgFunc from './js/dialog.js' //引入外部js\r\nimport axios from 'axios'\r\nexport default {\r\n  name: 'Dialog',\r\n  data(){\r\n      return {\r\n          DemoSpan : '',\r\n          docName: ''\r\n      }\r\n  },\r\n  methods:{\r\n      onbuttonclick(id){//调用js的点击事件\r\n          return dlgFunc.onbuttonclick(id)\r\n      },\r\n      onDocNameClick(){\r\n          this.docName = dlgFunc.onbuttonclick('getDocName')\r\n      },\r\n      onOpenWeb(){\r\n          dlgFunc.onbuttonclick('openWeb', this.DemoSpan)\r\n      }\r\n  },\r\n  mounted() {\r\n      axios.get('/.debugTemp/NotifyDemoUrl').then((res) => {\r\n          this.DemoSpan = res.data;\r\n      });\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.global{\r\n    font-size: 15px;\r\n    min-height: 95%;\r\n    }\r\n.divItem{\r\n    margin-left: 5px;\r\n    margin-bottom: 18px;\r\n    font-size: 15px;\r\n    word-wrap:break-word;\r\n    }\r\n</style>\r\n"]}]}