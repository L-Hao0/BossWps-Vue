>\r\n            <button style=\"margin:3px;\" @click=\"onDocNameClick()\">取文件名</button>\r\n        </div>\r\n        <hr>\r\n        <div class=\"divItem\">文档文件名为：<span>{{docName}}</span></div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport taskPane from './js/taskpane.js'\r\nexport default {\r\n  name: 'TaskPane',\r\n  data(){\r\n      return {\r\n          DemoSpan : '',\r\n          docName: ''\r\n      }\r\n  },\r\n  methods:{\r\n      onbuttonclick(id){\r\n          return taskPane.onbuttonclick(id)\r\n      },\r\n      onDocNameClick(){\r\n          this.docName = taskPane.onbuttonclick('getDocName')\r\n      },\r\n      onOpenWeb(){\r\n          taskPane.onbuttonclick('openWeb', this.DemoSpan)\r\n      }\r\n  },\r\n  mounted() {\r\n      axios.get('/.debugTemp/NotifyDemoUrl').then((res) => {\r\n          this.DemoSpan = res.data;\r\n      });\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.global{\r\n        font-size: 15px;\r\n        min-height: 95%;\r\n    }\r\n.divItem{\r\n    margin-left: 5px;\r\n    margin-bottom: 18px;\r\n    font-size: 15px;\r\n    word-wrap:break-word;\r\n}\r\n</style>\r\n"]}]}